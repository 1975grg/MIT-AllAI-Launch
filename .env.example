# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/allai_property

# Session Secret (Required for Authentication)
SESSION_SECRET=your-super-secret-session-key-here

# Replit Configuration (Automatically set in Replit environment)
REPL_ID=your-repl-id
REPLIT_DOMAINS=your-domain.replit.dev
ISSUER_URL=https://replit.com/oidc

# Optional: Google Places API (for enhanced address autocomplete)
GOOGLE_PLACES_API_KEY=your-google-places-api-key

# Address Provider Configuration
ADDRESS_PROVIDER=OSM  # Options: OSM (OpenStreetMap/Nominatim) or GOOGLE

# AI Agent Configuration (for future smart case features)
AGENT_BASE_URL=https://your-ai-agent.com/api
AGENT_API_KEY=your-ai-agent-api-key
WEBHOOK_SECRET=your-webhook-secret

# Email Configuration (for reminder notifications)
EMAIL_FROM=noreply@yourproperty.com
SMTP_URL=smtp://username:password@smtp.yourprovider.com:587

# Development/Production
NODE_ENV=development
PORT=5000
